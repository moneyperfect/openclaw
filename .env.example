# OpenClaw Gateway Configuration
# Copy this file to .env and fill in your values

# Base URL of your OpenClaw Gateway
VITE_OPENCLAW_BASE_URL=http://127.0.0.1:18789

# Your authentication token
VITE_OPENCLAW_TOKEN=your_token_here

# Set to 'true' to use fake adapter (mock responses), 'false' for real API
VITE_USE_FAKE_ADAPTER=true

# API endpoint for chat (if using HTTP mode)
# This will be appended to BASE_URL
VITE_CHAT_ENDPOINT=/api/chat
